<h1>Todos Index</h1>

<p>
<%= link_to "Create A Todo", new_todo_path %>
</p>

<table>
    <tr>
        <th>Name</th>
        <th>Description</th>
    </tr>
    <% @todos.each do |t| %>
        <tr>
            <td><%=t.name%></td>
            <td><%=t.description%></td>
            <td><%= link_to "Edit", edit_todo_path(t) %>
            <td><%= link_to "Show", todo_path(t) %>
        </tr>  
    <%end%>
</table>